program PrjCursoBasico;

uses
  Vcl.Forms,
  uFormsMain in 'Forms\uFormsMain.pas' {FormMain},
  uDmDados in 'Forms\uDmDados.pas' {DmDados: TDataModule},
  uBiblioteca in 'Classes\uBiblioteca.pas',
  uFormConfigBanco in 'Forms\uFormConfigBanco.pas' {FormConfigBanco},
  uFormsCadastroPai in 'Forms\uFormsCadastroPai.pas' {FormsCadastroPai},
  uFormsCadastroCliente in 'Forms\uFormsCadastroCliente.pas' {FormCadastroCliente},
  uFormsCadastroEstados in 'Forms\uFormsCadastroEstados.pas' {uFormsCadastroEstado};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormMain, FormMain);
  Application.CreateForm(TFormConfigBanco, FormConfigBanco);
  Application.CreateForm(TDmDados, DmDados);
  Application.CreateForm(TFormCadastroCliente, FormCadastroCliente);
  Application.CreateForm(TuFormsCadastroEstado, uFormsCadastroEstado);
  Application.Run;
end.
